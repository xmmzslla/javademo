package ncs.test07;

public class Student extends Human{
	String number;
	String major;
	
	Student() {
		
	}
	
	 Student(String name,int age, int height,int weight,
			 String number, String major) {
		 super();
			this.number = number;
			this.major = major;
	 }
	 
	 @Override
	public String toString(){
		 String data = name + "\t" + age + "\t" + height 
				 + "\t" + weight + "\t" + number + "\t" + major;
		 return data;
	 }
	 
	 


}
